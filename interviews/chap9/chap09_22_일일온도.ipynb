   "source": [
    "def dailyTemperatures(self, T):\n",
    "    stack = []\n",
    "    ans = [0]*len(T)\n",
    "    \n",
    "    for i, t in enumerate(T):\n",
    "        while stack and t>T[stack[-1]]:\n",
    "            last = stack.pop()\n",
    "            ans[last] = i - last\n",
    "        stack.append(i)\n",
    "        \n",
    "    return ans"
   ]
